box.pcr.galibang.cn {
    auto_https false
    gzip
    root /etc/caddy/www/vuepcr
    index index.html
    proxy /api box.pcr.galibang.cn/api/
    rewrite /api {
        regexp .*
        to box.pcr.galibang.cn/api/
    }
}
box.pcr.galibang.cn/api/ {
    auto_https false
    gzip
    proxy / 127.0.0.1:8081
}