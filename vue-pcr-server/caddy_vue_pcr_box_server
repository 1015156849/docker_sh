box.pcr.galibang.cn {
    gzip
    root /etc/caddy/www/vuepcr
    index index.html
    
    proxy /api 127.0.0.1:8081 {
        transparent
        
    }
    header /api {
            Access-Control-Allow-Credentials：true
            Access-control-allow-headers：*
            Access-Control-Allow-Origin  *
            Access-Control-Allow-Methods "GET, POST, OPTIONS"
    }
}